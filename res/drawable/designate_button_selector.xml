<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/w_b_transparent_300" />
            <stroke android:width="1.0dip" android:color="@color/primary_blue_300" />
            <corners android:radius="@dimen/tmap_8dp" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/primary_blue_100" />
            <corners android:radius="@dimen/tmap_8dp" />
            <stroke android:width="1.0dip" android:color="@color/primary_blue_300" />
        </shape>
    </item>
</selector>