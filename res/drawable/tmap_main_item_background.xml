<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/w_b_transparent_300" />
            <stroke android:width="@dimen/tmap_1dp" android:color="@color/color_dbdfe7" />
            <corners android:radius="@dimen/tmap_8dp" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/white_color" />
            <stroke android:width="@dimen/tmap_1dp" android:color="@color/color_dbdfe7" />
            <corners android:radius="@dimen/tmap_8dp" />
        </shape>
    </item>
</selector>