<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true">
        <shape>
            <solid android:color="@color/w_b_transparent_0" />
            <stroke android:width="1.0dip" android:color="@color/gray_900" />
            <corners android:radius="@dimen/tmap_8dp" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/w_b_transparent_0" />
            <stroke android:width="1.0dip" android:color="@color/semantic_red_500" />
            <corners android:radius="@dimen/tmap_8dp" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/w_b_transparent_0" />
            <stroke android:width="1.0dip" android:color="@color/gray_900" />
            <corners android:radius="@dimen/tmap_8dp" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/w_b_transparent_0" />
            <stroke android:width="1.0dip" android:color="@color/gray_300" />
            <corners android:radius="@dimen/tmap_8dp" />
        </shape>
    </item>
</selector>