<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/progress">
        <shape android:shape="ring" android:useLevel="false" android:innerRadius="@dimen/tmap_18dp" android:thickness="@dimen/tmap_2dp">
            <solid android:color="@color/color_33000000" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <rotate android:fromDegrees="270.0" android:toDegrees="270.0">
            <shape android:shape="ring" android:useLevel="true" android:innerRadius="@dimen/tmap_18dp" android:thickness="@dimen/tmap_2dp">
                <solid android:color="@color/color_000000" />
            </shape>
        </rotate>
    </item>
</layer-list>